#!/usr/bin/env ruby

require_relative '../lib/move.rb'

# Code your CLI Here
puts "Welcome to TicTacToe!"
board = [" ", " ", " ", " ", " ", " ", " ", " ", " ", ]
display_board(board)
puts "What move would you like to make?"
input = gets.strip
index = input_to_index(input)
move(board, index, team = "X")
display_board(board)
